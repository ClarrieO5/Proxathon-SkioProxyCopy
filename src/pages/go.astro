<!doctype html>
<html lang="en">
    <head>
        <script type="module">
            import Swal from "sweetalert2/dist/sweetalert2.js";
            import "sweetalert2/src/sweetalert2.scss";
            import tippy from "tippy.js";
            import "tippy.js/dist/tippy.css";
            function info() {
                Swal.fire({
                    icon: "info",
                    title: "Info",
                    text: "Horizon is a template/service for people to use for making proxy browsing easier. Simple settings and uses makes Horizon versatile for you to use at any time.",
                });
            }
        </script>
        <link
            href="https://fonts.cdnfonts.com/css/metropolis-2"
            rel="stylesheet"
        />
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <meta name="title" content="Horizon" />
        <meta
            name="description"
            content="A minimalistic Proxy with Everything you want. Made for Proxathon 2024"
        />
        <title>Google</title>
        <link rel="stylesheet" href="/assets/css/main.css" />
        <link
            rel="icon"
            href="https://www.google.com/favicon.ico"
            type="image/x-icon"
        />
        <style>
            iframe {
                display: block;
                background: #000;
                border: none;
                height: 100vh;
                width: 100%;
                position: fixed;
                inset: 0px;
                outline: none;
                overflow: auto;
                position: fixed;
                z-index: -1;
            }

            .gonav {
                display: flex;
                flex-wrap: wrap;
                flex-direction: row;
                width: 610px;
                height: 22px;
                border-radius: 20px;
                padding: 9px;
                position: fixed;
                top: 5.5vh;
                z-index: 10000;
                left: 50%;
                background: var(--base-color);
                box-shadow: 0 0 10px rgba(0, 0, 0, 0.59);
                backdrop-filter: blur(5px);
                text-align: center;
                align-content: center;
                align-items: center;
                transform: translate(-50%, -4vh);
            }

            .gonavAlign {
                text-align: center;
                position: absolute;
                left: 50%;
                transform: translate(-50%, 0);
                display: flex;
            }

            .gonavLink {
                height: 26px;
                border-radius: 20px;
                margin-left: 4px;
                margin-right: 4px;
                cursor: pointer;
                justify-content: flex-end;
                transition: 0.3s;
                padding: 5px;
            }

            .gonavLink:hover {
                transition: 0.3s;
                transform: translateY(-3px);
            }

            .gonavLink img {
                width: 26px;
                display: inline-flex;
                margin-left: -6px;
                margin-right: -6px;
                height: auto;
                transition: 1s;
                margin-top: 4px;
            }

            .gonavLink:hover img {
                animation: rotate 0.5s ease-in-out;
                transition: all 0.3s ease-in-out;
            }
        </style>
    </head>

    <body>
        <div class="gonav">
            <div class="gonavAlign">
                <a href="/">
                    <div class="gonavLink">
                        <img
                            style="animation: none;"
                            src="assets/imgs/goicons/home.png"
                        />
                    </div>
                </a>

                <a onclick="back()">
                    <div class="gonavLink">
                        <img
                            style="animation: none; margin-right: -10px;"
                            src="assets/imgs/goicons/backward.png"
                        />
                    </div>
                </a>

                <a onclick="reload()">
                    <div class="gonavLink">
                        <img
                            class="settingnav"
                            src="assets/imgs/goicons/reload.png"
                        />
                    </div>
                </a>

                <a onclick="forward()">
                    <div class="gonavLink">
                        <img
                            style="animation: none; margin-left: -10px;"
                            src="assets/imgs/goicons/forward.png"
                        />
                    </div>
                </a>

                <div
                    class="setting-input"
                    style="width: 400px; position: relative; margin-top: 7px;"
                >
                    <form id="uv-form">
                        <input
                            placeholder="Search or type a URL"
                            id="uv-address"
                            style="max-height: 10px; padding: 10px;"
                            class="setting-input__input"
                            type="text"
                        />
                    </form>
                </div>

                <a onclick="inspectelement()">
                    <div class="gonavLink">
                        <img
                            style="animation: none;"
                            src="assets/imgs/goicons/inspect.png"
                        />
                    </div>
                </a>

                <a onclick="newTab()">
                    <div class="gonavLink">
                        <img
                            style="animation: none;"
                            src="assets/imgs/goicons/new-tab.png"
                        />
                    </div>
                </a>

                <a onclick="toggleFullScreen()">
                    <div class="gonavLink">
                        <img
                            style="animation: none;"
                            src="assets/imgs/goicons/fullscreen.png"
                        />
                    </div>
                </a>
            </div>
        </div>

        <iframe
            src=""
            id="iframeId"
            style="bottom: 0;"
            style="border: 0px none;"
            name="Iframe"
            scrolling="yes"
            frameborder="0"
            marginheight="0px"
            marginwidth="0px"
            height="100%"
            width="100%"
            allowfullscreen></iframe>
    </body>
</html>
